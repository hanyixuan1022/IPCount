@page
@model ImageManagement.Pages.ImgUploadModel
@{
    ViewData["Title"] = "ImgUpload";
}

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <p>上传文件:</p>
        <input type="file" name="files" multiple="multiple" accept="image/png,image/jpeg,img/jpg" />
    </div>
    <div class="form-group">
        <input type="submit" value="上传" class="btn btn-primary" />
    </div>
</form>